<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myapp3">

    <uses-permission android:name="android.permission.SEND_SMS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".OptionsMenuActivity" />
        <activity android:name=".ProgressActivity" />
        <activity android:name=".AddNewAssessmentNoteActivity" />
        <activity android:name=".EditorAssessmentNoteActivity" />
        <activity android:name=".AssessmentNoteListActivity" />
        <activity android:name=".CourseAssessmentActivity" />
        <activity android:name=".EmailActivity" />
        <activity android:name=".TextActivity" />
        <activity android:name=".AddNewNoteActivity" />
        <activity android:name=".EditorNoteActivity" />
        <activity android:name=".NoteListActivity" />
        <activity android:name=".CourseNotes" />
        <activity android:name=".EditorAssessmentActivity" />
        <activity android:name=".AddNewAssessmentActivity" />
        <activity android:name=".EditorCourseActivity" />
        <activity android:name=".EditorTermActivity" />
        <activity android:name=".AddNewCourseActivity" />
        <activity android:name=".AddNewTermActivity" />
        <activity android:name=".AssessmentDetail" />
        <activity android:name=".TermDetail" />
        <activity android:name=".CourseDetail" />
        <activity android:name=".AssessmentList" />
        <activity android:name=".CourseList" />
        <activity android:name=".TermList" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name=".AlarmReceiver">
            <intent-filter>
                <action android:name="action.DISPLAY_NOTIFICATION" />
            </intent-filter>
        </receiver>
    </application>

</manifest>